<% form_for @place do %>

<%= text_field :model, :attribute %>

<%= select_tag "cplace_id", options_from_collection_for_select(Cplace.all, :id, :name) %>

<%= select_tag "cmoney_id", options_from_collection_for_select(Cmoney.all, :id, :name) %>

<%= select_tag "copen_id", options_from_collection_for_select(Copen.all, :id, :name) %>

<%= submit_tag "Crear" %>

<% end -%>


