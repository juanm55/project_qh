<div class="container-header">
  <table>
    <tr>
      <td valign="top"><%= link_to image_tag("logo.png", :alt => "queHago.co", :title => "Ir al inicio"), root_path %></td>

      <td width="622" align="right" valign="top"></td>
    </tr>
  </table>
  <% flash.each do |name, msg| %>
    <%= content_tag :div, msg, :id => "flash_#{name}" %>
  <% end %>
</div>

<% if false %>
  <nav class="round">
    <ul>
      <li><%= link_to "Inicio", root_path %></li>
      <li><%= link_to "Contacto", contact_path %></li>
      <li><%= link_to "Acerca de nosotros", about_path %></li>
    </ul>
  </nav>
<% end %>

